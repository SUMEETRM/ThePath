        System.out.println("chemistry-nanotechnology, chemical engineer, chemist");interests[5]=sc.nextInt();

        for(int i=0; i<6; i++)
        {

            if(interests[i]>10)interests[i]=10;
            if(interests[i]<0)interests[i]=0;
            interests[i]=interests[i]*10;
        }
        engineering+=interests[3]+(math/2)+(physics/2);
        physics=(physics/2)+(math/2)+interests[2];
        math+=interests[0];
        comp+=(math/2)+interests[1];
        
        
        
        
        bio=bio+interests[4];
        chem+=interests[5];
        
       
        int rm=0;
        do{
            System.out.println("CHOOSE YOUR ROLE MODEL");
            System.out.println("Albert einstien [1]       Stephen Hawking [2]      Bill Gates [3]     Steve jobs [4]     Elon Musk [5]      Jeff Bezos [6]");
