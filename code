            System.out.println("  Alfred Nobel  [14]    Marie Curie [15]       Pythagoras [16]         Alan Turing  [17]         APJ abdul Kalam [18]");

            rm=sc.nextInt();
            if(rm==1 || rm==2 || rm==7 || rm==8 ||  rm==18){physics+=30;}
          if(rm==3||rm==4||rm==5||rm==6||rm==9){comp+=30;}
            if(rm==10||rm==11||rm==12)bio+=30;
            if(rm==13 || rm==14||rm==15)chem+=30;
          if(rm==16||rm==17)math+=30;
              if(rm>18 && rm<1)System.out.println("Wrong choice");
        }while(rm>=18 && rm<=1);
System.out.println("\f");
