    void Arts(int iq3, String name3, String school3)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("                                         Enter % in various subjects");
         System.out.println("NOTE:marks>100 is considered as 100; marks<0 is considered as 0");
        System.out.println("english lit");double englishlit=sc.nextDouble();if(englishlit>100)englishlit=100;
        System.out.println("arts");double arts=sc.nextDouble();if(arts>100)arts=100;
        System.out.println("PE");double pe=sc.nextDouble();if(pe>100)pe=100;
        System.out.println("english lang");double englishlang=sc.nextDouble();if(englishlang>100)englishlang=100;
        System.out.println("TD");double td=sc.nextDouble();if(td>100)td=100;
        System.out.println("\f");

                
        System.out.println("NOTE:If the values entered exceed the upper limit, they will get the default max value"); 
        System.out.println("                                Please enter your interests from 1 to 10");
        System.out.println("Interior Design");double id=sc.nextDouble();if(id>10)id=10; id=id*10; id+=arts/2 + td/2; 
        System.out.println("Theatre");double t=sc.nextDouble();if(t>10)t=10; t=t*10; t+=arts/2 + englishlang/2;
        System.out.println("Artist");double a=sc.nextDouble();if(a>10)a=10; a=a*10; a+=arts;
        System.out.println("Literature");double lit=sc.nextDouble();if(lit>10)lit=10; lit=lit*10; lit+=englishlit/2 + englishlang/2; //from here
        System.out.println("LAW");double law=sc.nextDouble();if(law>10)law=10; law=law*10; law+=englishlit/2 + englishlang/2;;
        System.out.println("Sports Science and management");double sman=sc.nextDouble();if(sman>10)sman=10; sman=sman*10; sman+=pe;
        System.out.println("Event Management");double eman=sc.nextDouble();if(eman>10)eman=10; eman=eman*10; eman+=45 + englishlang/2;
        System.out.println("Fashion Design");double fd=sc.nextDouble();if(fd>10)fd=10; fd=fd*10; fd+=arts;
        System.out.println("architecture");double ar=sc.nextDouble();if(ar>10)ar=10; ar=ar*10; ar+=td;
        System.out.println("\f");
        int rm=0;

        do{
            System.out.println("CHOOSE YOUR ROLE MODEL");
            System.out.println(" Da Vinci[1]     Dan Brown [2]   JK Rowling(or some other author) [3]   some musician [4]   some actor [5] ");  
            System.out.println("some designer [6]     some builder [7]      some lawyer [8]       none[9]");

            rm=sc.nextInt();
            if(rm==1 )a+=30;
            if(rm==2 || rm==3)lit+=30;
            if(rm==4 || rm==5)t+=30;
            if(rm==6){fd+=30; id+=30;}
            if(rm==7)ar+=30;
            if(rm==8)law+=30;
            if(rm==9){a+=30; lit+=30; t+=30; fd+=30; id+=30; ar+=30; law+=30; eman+=30; sman+=30; }
        }while(rm>9 && rm<1);System.out.println("\f");

        int board2=0;
        do{
            System.out.println("Current situation");
            System.out.println("A-Under grad ABROAD-80% to 100%");
            System.out.println("B-Under grad in India 80% to 100%");

            System.out.println("C-Can't decide between abroad or India - 50/50");
            char go = sc.next().charAt(0);
            if(go=='a' || go=='A')board2=1;

            if(go=='b' || go=='B')board2=2;

            if(go=='c' || go=='C')board2=3;

        }while(board2 ==0); 
        System.out.println("\f");


                
        System.out.println("Generating report....................................................................");
        System.out.println("THE PATH");

        System.out.println("");
        System.out.println("");
        System.out.println("");
        System.out.println("");
         System.out.println("Subject chosen: ARTS                                    NAME:"+name3);

        System.out.println("Recommended options                                        School:"+school3);

        if(a>lit && a>sman && a>eman && a>t && a>fd && a>ar && a>law && a>id)
        {
            System.out.println("Pure Arts seems to be your main stength");
            System.out.println("artist");
            System.out.println("Graphic design, VFX");

        }

        if(lit>a && lit>sman && lit>eman && lit>t && lit>fd && lit>ar && lit>law && lit>id)
        {
            System.out.println("Literature seems to be your main strength");
            System.out.println("Writer");
            System.out.println("Author, poet, playright");

        }

        if(sman>lit && sman>a && sman>eman && sman>t && sman>fd && sman>ar && sman>law && sman>id)
        {
                        System.out.println("Sports and sports management seems to be your main strength");
            System.out.println("sports");
            System.out.println("Sports science");
            System.out.println("Sports management");

        }
        if(eman>lit && eman>sman && eman>a && eman>t && eman>fd && eman>ar && eman>law && eman>id)
        {
             System.out.println("Management seems to be your main strength");
