            if(go=='a' || go=='A')board1=1;

            if(go=='b' || go=='B')board1=2;

            if(go=='c' || go=='C')board1=3;

        }while(board1 ==0); 
        System.out.println("\f");
        System.out.println("Generating report....................................................................");
        for(long i =0; i <999999999; i++);
        {
            for(long j = 0; j <999999999; j++);
            {for(long k = 0; k<999999999; k++);
            }
        }
        System.out.println("\f");
        System.out.println("                                             THE PATH");

        System.out.println("                                              REPORT");
        System.out.println("");
        System.out.println("");
        System.out.println("");
        System.out.println("Subject chosen: COMMERCE                                    NAME:"+name1);

        System.out.println("Recommended options                                        School:"+school2);

        if(business>ent && business>law && business>data && business>manage  && business>acc && business>stock)
