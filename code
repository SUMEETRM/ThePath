        iq[1]=sc.nextInt();System.out.println("\f");
        System.out.println("                         3:The fifth consonant appearing at the beginning of this sentence is the letter i");
        System.out.println("                                          1-TRUE                       2-FALSE");iq[2]=sc.nextInt();System.out.println("\f");
        System.out.println("                         4:What number best completes the following series?   1, 9, 25, __, 81, 121");
        System.out.println("                                            36        45              64           49");iq[3]=sc.nextInt();System.out.println("\f");
        System.out.println("                5: If the second day of the month is a Monday, then the eighteenth day of the month is a Wednesday.");
        System.out.println("                                          1-TRUE                       2-FALSE");iq[4]=sc.nextInt();System.out.println("\f");
        System.out.println("                         6:If some Mees are Moos and all Moos are Yees, are all Yees also Mees?");
        System.out.println("                                                1- Yes                  2- NO");iq[5]=sc.nextInt();System.out.println("\f");
        System.out.println("                7:How many numbers are in the following series if all terms are included?0, 3, 6, 9, 12, 15, 18, ..., 960");iq[6]=sc.nextInt();System.out.println("\f");
        System.out.println("                 8:SAM, WHO IS 27 YEARS OLD, IS 3 TIMES OLDER THAN TODD. HOW OLD WILL SAM BE WHEN TODD IS 37?");iq[7]=sc.nextInt();System.out.println("\f");
        System.out.println(" 9:AFTER WALKING 3 BLOCKS EAST, THEN FOUR BLOCKS SOUTH, AND FINALLY THREE BLOCKS WEST, YOU WILL BE FOUR BLOCKS SOUTH OF YOUR STARTING POINT.");
        System.out.println("                                               1-TRUE                       2-FALSE");iq[8]=sc.nextInt();System.out.println("\f");

        System.out.println("\f");
        if(iq[0]==1)iqctr++;
        if(iq[1]==30)iqctr++;
        if(iq[2]==2)iqctr++;
        if(iq[3]==49)iqctr++;
        if(iq[4]==1)iqctr++;
        if(iq[5]==2)iqctr++;
        if(iq[6]==321)iqctr++;
        if(iq[7]==45)iqctr++;
        if(iq[8]==2)iqctr++;
        int mainch=0;
        do{  System.out.println("Select your choice.....");
            System.out.println("Science [1]");
            System.out.println("Commerce [2]");
            System.out.println("ARTS [3]");
            System.out.println("Haven't decided [4]");
            mainch=sc.nextInt();
            if(mainch==1)
            {obj.Science(iqctr, name, school);
            }
            if(mainch==2)
                obj.Commerce(iqctr, name, school);
            if(mainch==3)
                obj.Arts(iqctr, name, school);
            if(mainch==4)
                obj.IDK(iqctr, name, school);}while(mainch>=5 && mainch<=0);
    }
}

