        System.out.println("MATH");double math=sc.nextDouble(); if(math>100) math=100;
        System.out.println("physics");double physics=sc.nextDouble();if(physics>100) {physics=100;} 
        System.out.println("biology");double bio=sc.nextDouble(); if(bio>100) bio=100;
        System.out.println("chem");double chem=sc.nextDouble(); if(chem>100) chem=100;
        System.out.println("comp");double comp=sc.nextDouble();if(comp>100) {comp=100;}comp=comp/2;
        double engineering=0.0;
        System.out.println("\f");

        int interests[]=new int[6];
        System.out.println("Please enter your interest rating from 1 to 10");
        System.out.println("NOTE:interest>10 is considered as 10; interest<0 is considered as 0");
        System.out.println("mathematics related-acturial science, data science, pure mathematics etc....");interests[0]=sc.nextInt(); //190
        System.out.println("Computer related-computer researcher, software developer, IT, AI ML etc...");interests[1]=sc.nextInt(); 
