    void Commerce(int iq1, String name1, String school2)
    {
        Scanner sc=new Scanner(System.in);
        System.out.println("Enter % in various subjects");
        System.out.println("Note:Marks>100 is considered as 100 | marks<0 is considered as 0");
        System.out.println("MATH");double math=sc.nextDouble();if(math>100)math=100;
        System.out.println("economics");double economics=sc.nextDouble();if(economics>100)economics=100;
        System.out.println("Commercial application");double commercial=sc.nextDouble();if(commercial>100)commercial=100;
        System.out.println("English");double english=sc.nextDouble();if(english>100)english=100;

        double ent=(math/3)+economics/3+english/3;
        double business=(math/3)+economics/3+english/3;
        double stock=math;
        double acc=(math/3)+economics/3+commercial/3;
        double law=english;
        double data=math/2+economics/2;
        double manage=english/2+commercial/2;
        System.out.println("\f");

        int interests[]=new int[8];
System.out.println("rate your interests in the following from 0 to 10");
        System.out.println("NOTE:interest>10 is considered as 10; interest<0 is considered as 0");
        System.out.println("Business");interests[1]=sc.nextInt();
        System.out.println("Accountancy-CA, Income tax etc....");interests[2]=sc.nextInt();
        System.out.println("Stock Market");interests[3]=sc.nextInt();
        System.out.println("Actuarial Science");interests[4]=sc.nextInt();
        System.out.println("LAW");interests[5]=sc.nextInt();
        System.out.println("Entrepreneur");interests[6]=sc.nextInt();
        System.out.println("Management-hotel, events, etc.");interests[7]=sc.nextInt();
        for(int i=0; i<8; i++)
        {

            if(interests[i]>10)interests[i]=10;
            if(interests[i]<0)interests[i]=0;
            interests[i]=interests[i]*10;
        }
        business+=interests[1]*10;
        acc+=interests[2]*10;
        stock+=interests[3]*10;
        data+=interests[4]*10;
        law+=interests[5]*10;
        ent+=interests[6]*10;
        manage+=interests[7]*10;
        int rm=0;System.out.println("\f");

        do{
            System.out.println("CHOOSE YOUR ROLE MODEL");
            System.out.println(" Elon Musk [1]     Jeff Bezos  [2]      Warren Buffet [3]   narayana Murthy [4]   Mukesh Ambani[5]   Raghuram Rajan [6]   none [7]");

            rm=sc.nextInt();
            if(rm==1 || rm==2 ){ent+=30; manage+=30; data+=30;}
            if(rm==3){stock+=30; data+=30;}
            if(rm==4||rm==5)business+=30;
            if(rm==6){acc+=30;law+=30;}
            if(rm==7){acc+=30;ent+=30;stock+=30;business+=30; law+=30;   }
        }
        while(rm>7 && rm<1);System.out.println("\f");

       
        int board1=0;
        do{
            System.out.println("Current situation");
            System.out.println("A-Under grad ABROAD-80% to 100%");
            System.out.println("B-Under grad in India 80% to 100%");

            System.out.println("C-Can't decide between abroad or India - 50/50");
