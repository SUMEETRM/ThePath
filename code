        do{
            System.out.println("Current situation");
            System.out.println("A-Under grad ABROAD-80% to 100%");
            System.out.println("B-Under grad in India medical-80% to 100%");
            System.out.println("C-Under grad in India-IIT or top ranked private engineering - 80% to 100%");
            System.out.println("D-Can't decide between abroad or India - 50/50");
            System.out.println("Debug2"+engineering);
