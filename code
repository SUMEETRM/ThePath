        {
          System.out.println("Fashion Design seems to be your main strength");
            System.out.println("Fashion Design");
            System.out.println("VFX, Animation");

        }

        if(t>lit && t>sman && t>eman && t>a && t>fd && t>ar && t>law && t>id)
        {
            System.out.println("Theatre and performance arts seems to be your main strength");
            System.out.println(" Theatre-actor, musician, DJ etc.");
            

        }

        if(id>lit && id>sman && id>eman && id>a && id>fd && id>ar && id>law && id>t)
        {
            System.out.println("Design seems to be your main strength");
            System.out.println("Interior design");
           System.out.println("Architecture");

        }

